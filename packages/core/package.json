{
  "name": "@authjs-web3-providers/core",
  "version": "0.4.1",
  "description": "Contains base abstractions and utilities for AuthJS Web3 Providers",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p .",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "docs": "typedoc --out docs src"
  },
  "dependencies": {
    "cookie": "^1.0.2",
    "next-auth": "catalog:next-auth-4",
    "siwe": "catalog:siwe-3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.24.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "auth",
    "web3",
    "ethereum",
    "siwe",
    "next-auth",
    "authentication"
  ],
  "author": "palsergech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palsergech/authjs-web3-providers.git"
  },
  "bugs": {
    "url": "https://github.com/palsergech/authjs-web3-providers/issues"
  },
  "homepage": "https://github.com/palsergech/authjs-web3-providers#readme",
  "publishConfig": {
    "access": "public"
  }
}
